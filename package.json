{
  "name": "@velocitycubed/capacitor-plugin-msauth-v2",
  "version": "4.0.0-5",
  "description": "This plugin enables MSAL support for Capacitor.",
  "main": "dist/plugin.cjs.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/plugin.js",
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "dist/",
    "ios/Plugin/",
    "VelocitycubedCapacitorPluginMsauthV2.podspec"
  ],
  "author": "Velocity",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/VelocityCubed/capacitor-plugin-msauth-v2.git"
  },
  "bugs": {
    "url": "https://github.com/VelocityCubed/capacitor-plugin-msauth-v2/issues"
  },
  "keywords": [
    "capacitor",
    "plugin",
    "native",
    "msal",
    "microsoft",
    "azure",
    "authentication"
  ],
  "scripts": {
    "verify": "npm run verify:ios && npm run verify:android && npm run verify:web",
    "verify:ios": "cd ios && pod install && xcodebuild -workspace Plugin.xcworkspace -scheme Plugin -destination generic/platform=iOS && cd ..",
    "verify:android": "cd android && ./gradlew clean build test && cd ..",
    "verify:web": "npm run build",
    "lint": "npm run eslint && npm run prettier -- --check && npm run swiftlint -- lint",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write && npm run swiftlint -- --fix --format",
    "eslint": "eslint . --ext ts",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\" --plugin=prettier-plugin-java",
    "swiftlint": "node-swiftlint",
    "build": "npm run clean && tsc && rollup -c rollup.config.mjs",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run fmt && npm run lint && npm run build",
    "precommit": "npm run fmt && npm run lint && npm run build"
  },
  "devDependencies": {
    "@capacitor/android": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/ios": "^6.0.0",
    "@ionic/eslint-config": "^0.4.0",
    "@ionic/prettier-config": "^4.0.0",
    "@ionic/swiftlint-config": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "eslint": "^8.57.0",
    "precommit": "^1.2.2",
    "prettier": "^3.4.2",
    "prettier-plugin-java": "^2.6.6",
    "rimraf": "^6.0.1",
    "rollup": "^4.30.1",
    "swiftlint": "^2.0.0",
    "typescript": "~5.7.3"
  },
  "peerDependencies": {
    "@capacitor/core": ">=6.0.0"
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "dependencies": {
    "@azure/msal-browser": "^2.32.2"
  },
  "eslintConfig": {
    "extends": "@ionic/eslint-config/recommended"
  },
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "precommit": {
    "tasks": [
      "npm run precommit"
    ]
  }
}